{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@assets/*": ["src/assets/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@interfaces": ["src/interfaces"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"],
      "@config/*": ["src/config/*"],
      "@config": ["src/config"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"],
      "@styles": ["src/styles"],
      "@styles/*": ["src/styles/*"]
    }
  }
}
